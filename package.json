{
  "name": "vice-ui-server",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "concurrently \"node start-loading-ui.js\" \"node start-landing-ui.js\"",
    "server": "concurrently \"npm run port-forward\" \"node start-server.js\"",
    "port-forward" : "node -r dotenv/config port-forward.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "concurrently": "^4.0.1",
    "dotenv": "^6.1.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.3"
  }
}
